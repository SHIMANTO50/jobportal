<!DOCTYPE html>
<html>
<head>
	<title>Home Page</title>
</head>
<body>
	<div>
		<h1>User List</h1> 		
	</div>

	<a href="/home"> Back</a> |
	<a href="/logout"> logout</a><br>

	<table border="1">
		<tr>
			<td>adminid</td>
			<td>Adminname</td>
			<td>Companyname</td>
			<td>ContactNo</td>
			<td>UserName</td>
			<td>Password</td>
		</tr>
		<% for(var i=0; i < userlist.length; i++){ %>
		<tr>
			<td><%= userlist[i].aid %></td>
			<td><%= userlist[i].adminname %></td>
			<td><%= userlist[i].companyname %></td>
			<td><%= userlist[i].contactno %></td>
			<td><%= userlist[i].username %></td>
			<td><%= userlist[i].password %></td>
			
			<td>
				<a href="/home/edit/<%= userlist[i].id %>">EDIT</a> | 
				<a href="/home/delete/<%= userlist[i].id %>">DELETE</a>  
			</td>
		</tr>
		<% } %>
	</table>
</body>
</html>